 <?php 
                    function results($results)
                    {
                        $string = "";
                        
                        foreach ($results as $skill => $node)                      
                        {
                            $string = "name:'" . $skill . "',\ndata:";
                        
                            foreach ($node as $result)
                            {
                                $string .= ("[Date.UTC(" . str_replace("-", ",", $result["date"]) . "), " . $result["user_grade"] . "], \n");                       
                            }  
                            
                            $string = substr("$string", 0, -2);
                            
                        }
                        
                        $string .= "], ";
                        
                        print($string);
                    }
                    //dump(results("COMPRENDRE", $results));
                    ?>  
   <main class="col-md-10">         
       
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script src="http://code.highcharts.com/modules/exporting.js"></script>

    <script>
            $(function () {
                $('#chart1').highcharts({
                    chart: {
                        type: 'line'
                    },
                    title: {
                        text: 'Comp√©tences'
                    },
                    subtitle: {
                        text: ''
                    },
                    xAxis: {
                        type: 'datetime',
                        dateTimeLabelFormats: { // don't display the dummy year
                        month: '%b',
                        year: '%y'
                        }
                        
                    },
                    yAxis: {
                        title: {
                            text: 'Pourcentage'
                        }
                    },
                    plotOptions: {
                        line: {
                            dataLabels: {
                                enabled: true
                            },
                            enableMouseTracking: true
                        }
                    },
                    series: [{
                        
                        <?= results($results); ?>
                    }, {
                        name: 'COMPRENDRE',
                        data: [
                        ]
                    }]
                });
        });
    </script>

    <div id="chart1" class="col-centered"></div>

     
                    
    
    </main>
